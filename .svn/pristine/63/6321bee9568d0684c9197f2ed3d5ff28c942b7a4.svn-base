.notice-font {float:right;color:#5ec3b8;}
.flightcheckin-half-round {position: absolute;bottom: 102px;left: -3px;z-index: 999;width: 15px;height: 25px;background-color: #efeff4;border-radius: 0px 10px 10px 0px;padding: 0;}
.flightcheckin-msg{text-align: center;padding: 0;}
.travelnum-font {display:inline-block;}
.edit-usernum {display:none;}
.CancelCheckin{background: #ffc550;}
.reserveSeat{background: #ffc550;}
.Checkin-color{background: #89B3EF;}
.unavailable-color{background: #bdc1cd;}
.follow-flight{background: #5ec3b8;}
.flightcheckin-firstdiv{width: 100%;position: absolute;top: 0;left: 0;height: 26px;color: #666;border-bottom: 1px solid #ccc;}
.checkin-left{color:#ccc;}
.checkin-right{color:#ccc;}
.checkin-name-font{font-size:12px;margin: 5px auto;text-align: center;}
.lyb-content-item2 li{display: inline-block;position: relative;width: 50px;height: 60px;}
.remove-location{position: absolute;top: 0px;left: 31px;}
.head-num{font-size: 12px;position: absolute;top: 20px;left: 22px;color: #999;}
.others{width: 95%;margin: 0 auto;font-size: 14px;}
.others li{display: inline-block;}
.others-font{color: #999;}
.price{  display: inline-block;color: #5ec3b8;}
/* flightcheckin end*/

/* seatselectdetails start*/
.seatdetails-footer{background-color:#5ec3b8;width:100%;min-height:90px;}
/* seatselectdetails end*/ 

/*Recommend seat window start*/
.recommend-window{width: 90%;min-height:155px;background-color: rgba(0,0,0,0.8);position: absolute;top: 50px;left: 0px;z-index: 1000;border-radius: 5px;}
.recommend-window-left{float:left;}
.window-left-title{color: #5ec3b8;margin-left: 45px;margin-top: 23px;margin-bottom: 26px;}
.window-left-li{color: #5ec3b8;margin-left: 45px;margin-bottom: 13px;position: relative;}
.recommend-window-right{float:right;}
.recommend-img{background-position:-198px -2px;width: 80px;height: 98px;}
.recommend-img-margin{margin-top: 23px;margin-right: 49px;}
.recommend-num{display: inline-block;margin-left: 8px;color: #ffcc66;font-size: 18px;}
.recommend-li-div{  display: inline-block;}
.window-circle{position: absolute;bottom: 5px;border-radius: 50%;width: 10px;height: 10px;background-color: #5ec3b8;z-index: 999;}
.recommend-window-text{margin-left: 16px;}
/*Recommend seat window end*/

/*seat start*/
div.seatCharts-cell {color: #bebebe;font-family: "黑体";height: 26px;width: 26px;line-height: 26px;margin: 3px;float: left;text-align: center;outline: none;font-size: 13px;}
div.seatCharts-seat.available {/* background-color: #B9DEA0; */background-position: -96px -136px;width: 26px;height: 26px;}
div.seatCharts-seat.unavailable {/* 	background-color: #ccc; */background-position: -96px -104px;width: 26px;height: 26px;cursor: not-allowed;}
div.seatCharts-seat {background: url(../images/allicon.png) no-repeat;background-size: 400px 750px;color: #FFFFFF;cursor: pointer;}
div.seatCharts-seat.selected {/* background-color: #E6CAC4; */background-position: -1px -105px;width: 26px;height: 26px;}
div.seatCharts-seat.focused {/* background-color: #76B474; */}
div.seatCharts-space {background-color: #32343c;}
div.seatCharts-row {height: 35px;}
div.seatCharts-seat.recommended  {/* background-color: #ffff80; */background-position: -1px -136px;width: 26px;height: 26px;}
div.seatCharts-container {/* 	border-right: 1px dotted #adadad; */min-width: 310px;width: 100%;padding: 20px;float: left;}
.seat-map{background-color: #32343c;}
.seat-explain{float:left;margin-top: 20px;margin-left: 48px;}
.seat-explain-font{float:left;color:#bebebe;margin-left: 20px;}
.Emergency-Exit-leftline{display: inline-block;height:1px;border:none;border-top:1px solid #996666;width: 30%;margin: 0 6%;}
.Emergency-Exit-text{display: inline-block;color: #996666;font-size: 12px;margin-bottom: -6px;width: 16%;text-align: center;}
.floor-button{position: relative;width:70px;height:20px;  background-color: #585c68;border-radius: 10px;text-align: center;line-height: 20px;color: #d0d0d0;}
.floor-button-circle{position: absolute;bottom: 1px;border-radius: 50%;width: 18px;height: 18px;background-color: #5ec3b8;z-index: 999;}
.floor-button .left{left: 49px;}
.floor-button .right{right: 49px;}
/*seat end*/