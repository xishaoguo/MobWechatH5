.trash-icon{background-position:-4px -356px;width: 14px;height: 16px;margin-left: 10px;}/*垃圾桶*/
.checkboxed-icon{background-position:-1px -398px;width: 16px;height: 13px;}/*选中对号*/
.guide-plane-icon{background-position:-80px -291px;width: 18px;height: 16px;}/*行程轨迹飞机图标*/
.guide-seat-icon{background-position:-4px -271px;width: 13px;height: 15px;top:199px;}/*选座*/
.guide-checkin-icon{background-position:-80px -272px;width: 16px;height: 14px;}/*值机*/
.guide-consignment-icon{background-position:-80px -251px;width: 16px;height: 12px;}/*托运*/
.guide-check-icon{background-position:-145px -251px;width: 13px;height: 15px;}/*安检*/
.guide-boarding-icon{background-position:-4px -294px;width: 15px;height: 10px;}/*登机*/
.guide-arrive-icon{background-position:-4px -257px;width: 15px;height: 9px;}/*到达*/
.display-icon{display: inline-block;margin-right: 6px;}
.write-icon{background-position:-4px -330px;width: 17px;height: 18px;display: inline-block;margin-right: 7px;}/*编辑文本图标*/
.lphoto-icon{background-position:-80px -496px;width: 26px;height: 26px;display: inline-block;margin:0 6px;}/*小相机*/
.bphoto-icon{background-position:-80px -440px;width: 41px;height: 41px;position: absolute;position: absolute;right: 48px;top: 178px;}/*大相机*/
.authentication-success-icon{background-position:-4px -437px;width: 50px;height: 50px;margin: 97px auto 16px;}/*认证成功图标*/
.exclamation-mark-icon{background-position:-4px -557px;width: 50px;height: 50px;margin: 97px auto 16px;}/*感叹号*/
.lyb-icon{width: 44px;height: 44px;background: url(../images/icon.png) no-repeat; background-size: 132px 2728px;}
.lyb-forward-img{width: 30px;background-position: -7px -608px; display:inline-block; vertical-align: middle;float: right;position: absolute;top: 94px;right: 6px;}
.selected-head-portrait{background-position:-151px -613px;width: 29px;height: 29px;margin: 0 auto;}/*选中头像*/
.select-head-portrait{background-position:-4px -614px;width: 29px;height: 29px;margin: 0 auto;}/*未选中头像*/
.checkin-remove-user{background-position:-81px -529px;width: 16px;height: 16px;}/*减号*/
/*icon img end*/

/*seat number header start*/
.lyb-seatnumber-header{height: 35px;line-height: 10px;background-color: #ffffff!important;border-style: none;}
.lyb-div{margin-top:15px;}
.lyb-seatnumber-font1{color:#575757;font-size: 18px;font-weight: bold;padding-top:10px;padding-left:7px;float: left;}
.lyb-seatnumber-font2{color:#ff8040;font-size: 20px;font-weight: bold;padding-top:10px;padding-left:7px;float: left;}
.lyb-seatnumber-button{height: 23px;width:50px;background-color: #ff8040;margin-top:15px;margin-right:7px;float: right;border-radius:5px;}
.lyb-seatnumber-font3{line-height: 23px;color:#ffffff;font-size: 10px;font-weight: bold;text-align: center;}
/*may know people start*/
.lyb-mayknow{height: 120px;line-height: 10px;border-top-style:solid;border-bottom-style:solid;border-width: 1px;border-color:#999999;background-color: #ffffff!important;}


.footer{width:100%;position:absolute;bottom:0px;left:0px;}
.footer .button{width:100%;height:48px;background-color:#5ec3b8;color:#fff;font-family: "黑体";font-size:17px;font-weight:normal;text-align:center;line-height:48px;border:none;padding:0;}
.footer .tocard{width:80%;height:40px;background: -webkit-linear-gradient(left, #5ec3b8, #abd99d);color:#fff;font-size:17px;font-weight:bold;text-align:center;line-height:40px;border:none;padding:0;margin:20px auto;border-radius: 5px;}
.footer .copyright{width:100%;height:48px;background-color:#fff;color:#666;font-family: "黑体";font-size:17px;font-weight:normal;text-align:center;line-height:48px;border:none;padding:0;}
.seat {width: 100%;text-align: center;margin: 0 auto;}
/* loading begin */
.loading{width:110px;min-height:95px;position:absolute;left:50%;top:50%;background-color: #ccc;border-radius:4px;padding-bottom:5px;}
.loading .loading-gif{width:50px;height:50px;background:url(../images/loading.gif) no-repeat;background-size:50px 50px;margin:10px auto 0;}
.loading .loading-title{width:110px;min-height:20px;line-height:20px;color:#fff;font-size:17px;font-weight:normal;text-align:center;margin-top:10px;}
.loading-mask{width:100%;height:100%;position:absolute;top:0;left:0;z-index:999;}
/* loading end */

/* alert begin */
.alert-box{width:80%;font-family: "黑体";/*min-height:30px;*/background-color:#fff;position:absolute;top:50px;left:0px;z-index:1000;}
.alert-box .alert-title{text-align:center;font-size:18px;color:#000;font-weight:bold;line-height:20px;/*padding:5px;*/padding-top:20px;}
.alert-box .alert-content{display:table-cell;padding:15px 12px 30px 12px;word-break:break-word;vertical-align:middle;/*padding:5px 10px;*/border-bottom:1px solid #dcdcdc;color:#666;text-align:center;/*min-height:25px;line-height:20px;*/}
.alert-box .alert-content p{display:table-cell;vertical-align:middle;}
.alert-box .alert-btn-cancel{color:#fff;border:none;box-shadow:none;border-radius:0;background-color:#ccc;}
.alert-box .alert-btn-cancel:active,.alert-box .alert-btn-sure:active{background-color:#5ec3b8;color:#fff;}
.alert-box .alert-btn-sure{color:#fff;border:none;box-shadow:none;border-radius:0;}
.alert-box .ui-grid-a div:nth-child(2n){border-left:1px solid #dcdcdc;}
.alert-box button{padding:0.5em 1em;margin:0;height:44px;background-color:#5ec3b8;}
.alert-box .alert-wrapper{min-height:95px;}
.alert-box .alert-content-wrapper{display:table;width:100%;}
/* alert end */
/* 毛玻璃效果css */
.frosted-glass{-webkit-filter:blur(3px);-moz-filter:blur(3px);-ms-filter:blur(3px);filter:blur(3px)}
/* 遮罩层class */
.select-mask{width:100%;height:100%;background:rgba(0,0,0,0.3);position:absolute;top:0;left:0;z-index:999;}

