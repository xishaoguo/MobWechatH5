.psg-info em{ background-color: #FFF;right: 24px;position: absolute;line-height: 35px;text-align: right;}
.psg-info input{ background-color: #FFF;right: 24px;position: absolute;line-height: 35px;text-align: right;}
.panel-scroll{ position:absolute; z-index:1; width:100%; overflow: hidden;}
.panel-wrapper{position:absolute; z-index:1; bottom:0px;width:100%; overflow: hidden;}

/*-----------------------------purpose ---------------------------------------*/
.other-reason{height: 150px;width: 100%;border: 0px;text-align: left;margin-top: 10px;border: 1px solid #DCDCDC;}
.textarea-reason{background-color: #FFF;width: 100%;height: 100%}

/*------------------------------amex submit-----------------------------------------*/
.amex-submit-item{ line-height: 44px;}
.amex-submit-item div{ text-indent: 12px;width: 90%;}
.amex-submit-item span{ padding: 0px 10px;position: absolute;right: 20px;text-align: right;width: 65%;}
.amex-bar-disc{text-indent: 12px;height: 32px;line-height: 32px;}
.amex-appr-model{border-bottom: 1px #DCDCDC solid;border-top: 1px #DCDCDC solid;margin-bottom: 20px;}

/**---------------------------------列表 说明 描述bar 如违背原因，历史，------------------------------------*/
.description-bar{text-indent: 10px;height: 32px;line-height: 32px;background-color: #F3F3F3;border: 1px #DCDCDC;margin-left: 12px;}
.single-list-h{height: 44px !important;line-height:44px !important;}
.doubli-list-h{height: 64px;line-height:64px;}

